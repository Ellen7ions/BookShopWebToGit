    <%
        if (request.getAttribute("loginFailPwd") != null) {
    %>
    <div class="alert alert-warning">
        <strong>登陆失败！</strong> 密码不正确！
    </div>
    <%
            request.removeAttribute("loginFailPwd");
        }
    %>

    <%
        if (request.getAttribute("userExists") != null) {
    %>
    <div class="alert alert-warning">
        <strong>注册失败！</strong> 用户已存在！请登录！
    </div>
    <%
            request.removeAttribute("userExists");
        }
