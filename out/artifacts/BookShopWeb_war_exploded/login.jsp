    <script type="text/javascript">
        function check(form) {
            console.log("歪比歪比?");
            if (form.email.value == "") {
                alert("邮箱不能为空！");
                return false;
            } else if (form.pwd.value == "") {
                alert("密码不能为空！");
                return false;
            }
            return true;
        }
